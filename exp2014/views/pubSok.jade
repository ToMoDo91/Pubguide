extends layout

block content
    #wrapper
      header
        h1 Project Bar-Berg
      #hoved
        #nav-bar
          a(href='/') Hjem
          a(href='pubListe') Pub
          a(href='') Om
          a(href='') Kontakt
          #sok
            p SÃ¸k etter en bar i Bergen
            form(method='get', action='pubSok')
              input(type='text', name='sok')


        br
        h2 Liste over pub i Bergen
        #hovedinnhold
          p Noe annet
          #sider
            -var i = 0;
            #mask
              mixin sider(id,navn,n,j,href)
                div(class='side')&attributes(attributes)
                  .sideDiv
                    h2= navn
                    - for (; i < n; i++)
                      a(href="pub?#{uriStr[i]}")
                        section
                          h4= liste[i].pub
                          p(class='type') Type #{liste[i].type}
                          p(class='alder') Alder: #{liste[i].alder}
                          img(src='/images/fff.png')
                        br

                  .sideDiv2
                    h2= navn
                    - for (; i < j; i++)
                      a(href="pub?#{uriStr[i]}")
                        section
                          h4= liste[i].pub
                          p(class='type') Type: #{liste[i].type}
                          p(class='alder') Alder: #{liste[i].alder}
                          img(src='/images/fff.png')
                        br
                  .atag
                    a(href=href).panel Neste side
              if lengde < 6
                +sider(side1,1,lengde,0,'#side2')(id='side1')

              else if 5 < lengde && lengde < 11
                +sider(side2,2,5,lengde,'#side2')(id='side1')

              else if 10 < lengde && lengde < 16
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,lengde,0,'#side3')(id='side2')

              else if 10 < lengde && lengde < 21
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,lengde,'#side3')(id='side2')

              else if 20 < lengde && lengde < 26
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,lengde,0,'#side4')(id='side3')

              else if 20 < lengde && lengde < 31
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,lengde,'#side4')(id='side3')

              else if 30 < lengde && lengde < 36
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,lengde,0,'#side5')(id='side4')

              else if 30 < lengde && lengde < 41
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,lengde,'#side5')(id='side4')

              else if 40 < lengde && lengde < 46
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,40,'#side5')(id='side4')
                +sider(side5,5,lengde,0,'#side6')(id='side5')

              else if 40 < lengde && lengde < 51
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,40,'#side5')(id='side4')
                +sider(side5,5,45,lengde,'#side6')(id='side5')

              else if 50 < lengde && lengde < 56
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,40,'#side5')(id='side4')
                +sider(side5,5,45,50,'#side6')(id='side5')
                +sider(side6,6,lengde,0,'#side7')(id='side6')

              else if 50 < lengde && lengde < 61
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,40,'#side5')(id='side4')
                +sider(side5,5,45,50,'#side6')(id='side5')
                +sider(side6,6,55,lengde,'#side7')(id='side6')

              else if 60 < lengde && lengde < 66
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,40,'#side5')(id='side4')
                +sider(side5,5,45,50,'#side6')(id='side5')
                +sider(side6,6,55,60,'#side7')(id='side6')
                +sider(side7,7,lengde,0,'#side1')(id='side7')

              else if 60 < lengde && lengde < 71
                +sider(side2,2,5,10,'#side2')(id='side1')
                +sider(side3,3,15,20,'#side3')(id='side2')
                +sider(side3,3,25,30,'#side4')(id='side3')
                +sider(side4,4,35,40,'#side5')(id='side4')
                +sider(side5,5,45,50,'#side6')(id='side5')
                +sider(side6,6,55,60,'#side7')(id='side6')
                +sider(side7,7,65,lengde,'#side1')(id='side7')
